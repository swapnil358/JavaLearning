package TestPackage;

public class LargestAndSmallestOfArray {

	
	
		
			
	public static void main(String[] args) {
		
		int[] a = new int[] { 1, 20, 15, 50, 90 };
		
		
						
		int min = a[0];
		
		int max = a[0];
		
		for(int i=0; i<a.length; i++ ) {
			
			if(a[i]>max) {
				
				max = a[i];	
				
				System.out.println("max"+max);
			}
			if(a[i]>max) {
				
				max = a[i];
				
				System.out.println("Max"+max);
			}
			
			
			
			
		}
		
		
		

	}

}
