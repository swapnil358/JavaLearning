package DataStructure;

public class StackImp {
	
	//1. Initialising three variable, Size--Size of stack, arr[]--array of stack, top -- array present on top position

	int size;
	int arr[];
	int top;
	
	//initialise the constructor with size 
	
	public StackImp(int size) {
		
		this.size = size;
		this.arr[] = new int[size];
		
	}
	
	
	

}
